[project]
name = "libmoon"
version = "0.0.25"
authors = [
  { name="Xiaoyuan Zhang", email="xzhang2523-c@my.cityu.edu.hk" },
]
description = "Moon: A small step of MOO, a big step for the human. We made this project is not because MOO is simple, but MOO is hard."


readme = "README.md"
requires-python = ">=3.11"
classifiers = [
    "Programming Language :: Python :: 3",
    "License :: OSI Approved :: MIT License",
    "Operating System :: OS Independent",
]

dependencies=[
    'numpy>=1.26.4',
    'torch>=2.2.1',
    'matplotlib>=3.8.3',
    'tqdm>=4.66.2',
    'pymoo>=0.6.1.1',
    'cvxopt>=1.3.2',
    'cvxpy>=1.4.2'
]


[project.urls]
Homepage = "https://github.com/xzhang2523/libmoon"
Issues = "https://github.com/xzhang2523/libmoon/issues"

[build-system]
requires = ["setuptools >= 61.0"]
build-backend = "setuptools.build_meta"
